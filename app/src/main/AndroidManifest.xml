<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidapp1d">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudFeedActivity" />
        <activity
            android:name=".StudNotificationActivity"
            android:parentActivityName=".StudFeedActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StudFeedActivity" />
        </activity>
        <activity android:name=".StudBookingActivity"></activity>
        <activity android:name=".StudProfileActivity"></activity>
        <activity android:name=".ProfFeedActivity"></activity>
        <activity android:name=".StudSearchActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".StudBookingProf"></activity>
        <activity android:name=".StudBookingMod"></activity>

        <activity android:name=".ProfBookingActivity" />
        <activity android:name=".ProfNotificationActivity" />
        <activity android:name=".ProfProfileActivity" />
        <activity android:name=".ProfSearchActivity" />
        <activity android:name=".ProfPersonalInfoPage" />
        <activity android:name=".ProfReportStudentsPage" />
        <activity android:name=".ProfBookingsPage" />
        <activity android:name=".ProfChangePWpage" />
        <activity android:name=".ProfModulesList" />
        <activity android:name=".ProfPriorityListPage"></activity>
    </application>

</manifest>